---
import { getCollection } from 'astro:content'
import Layout from '../layouts/Layout.astro'

export async function getStaticPaths({ paginate }: { paginate: Function }) {
  const posts = await getCollection('posts')
  return paginate(posts, {
    pageSize: 1,
  })
}
